<!DOCTYPE html>
<html>
<head>
  <title>Eventful</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="container">
  	<div class="row">
    	<div class="col-sm-12">
					<%= render 'layouts/navbar' %>
					<% flash.each do |key, value| %>
						<%= value %>
					<% end %>
 			</div>
    </div>
  </div>
</body>
</html>

<%= yield %>

</body>
</html>
